{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "roots": ["<rootDir>"],
  "transform": {
    "\\.spec.[jt]sx?$": [
      "@swc/jest",
      {
        "jsc": {
          "transform": {
            "react": {
              "runtime": "automatic"
            }
          }
        }
      }
    ]
  },
  "moduleNameMapper": { "\\.(css|sass)$": "identity-obj-proxy" }
}
